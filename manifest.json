{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BetterDL 1.3 - Improve DLive.tv",
  "version": "1.3",
  "manifest_version": 2,
  "background": {
    "scripts":["background.js"]
  },
  "web_accessible_resources": ["inject.css", "inject.js","emotes.js", "modules/*"],
  "content_scripts": [
    {
      "matches": ["https://dlive.tv/*"],
      "js": ["content.js"],
      "css": ["inject.css"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}